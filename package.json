{
	"name": "covau",
	"version": "0.0.1",
	"scripts": {
		"netlify-dev": "cd functions && netlify dev",
		"netlify-serve": "cd functions && netlify serve",
		"netlify-deploy": "npm run build && cd functions && netlify deploy",
		"netlify-deploy-prod": "npm run build && cd functions && netlify deploy --prod",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-netlify": "^2.0.6",
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.5.0",
		"@types/cookie": "^0.5.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@netlify/functions": "^1.4.0",
		"@types/youtube": "^0.0.47",
		"async-mutex": "^0.4.0",
		"dashjs": "^4.6.0",
		"firebase": "^9.22.0",
		"youtubei.js": "^4.0.1"
	}
}
